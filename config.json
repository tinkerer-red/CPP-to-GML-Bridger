{
  "include_files": [
    "input/include/openxr/openxr.h"
  ],
  "libraries": [
    "input/lib/openxr_loader.lib"
  ],
  
  "output_folder": "output",
  "output_cpp_file": "bridge.cpp",
  
  "gml_stub": "GM_OpenXR.gml",
  "yy_extension": "GM_OpenXR.yy",
  
  "project_name": "GM_OpenXR",
  "namespace": "XR",
  "init_function": "YYExtensionInitialise",
  
  "skip_function_prefixes": ["_"],
  "cull_function_names": true,
  "cull_constant_names": true,
  "cull_enum_names": true,
  
  "debug": true,
  
  "preprocessor": ["cpp", "-P", "-dD", "-std=c99"],
  "preprocessor_defines": [
    "XR_EXTENSION_PROTOTYPES"
  ]
  
}
